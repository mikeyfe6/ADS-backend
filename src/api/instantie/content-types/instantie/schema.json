{
  "kind": "collectionType",
  "collectionName": "instanties",
  "info": {
    "singularName": "instantie",
    "pluralName": "instanties",
    "displayName": "Instanties",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "profile": {
      "type": "string"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "background": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "bgfree": {
      "type": "enumeration",
      "enum": [
        "zwart",
        "grijs",
        "geel",
        "roze",
        "bruin",
        "groen",
        "afrotheme"
      ],
      "default": "zwart"
    },
    "facebooklink": {
      "type": "string"
    },
    "twitterlink": {
      "type": "string"
    },
    "instagramlink": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "whatsapplink": {
      "type": "string"
    },
    "tiktoklink": {
      "type": "string"
    },
    "persoon": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "website"
    },
    "occupate": {
      "type": "enumeration",
      "enum": [
        "bedrijf",
        "zelfstandig",
        "hobbyist",
        "stichting",
        "artist"
      ]
    },
    "pinterestlink": {
      "type": "string"
    },
    "snapchatlink": {
      "type": "string"
    },
    "linkedinlink": {
      "type": "string"
    },
    "youtubelink": {
      "type": "string"
    },
    "gofundmelink": {
      "type": "string"
    },
    "biography": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "light"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "address": {
      "displayName": "Text & Boolean",
      "type": "component",
      "repeatable": false,
      "component": "conditional.text-and-boolean"
    }
  }
}
